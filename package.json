{
  "name": "@lbwa/tsconfig",
  "version": "1.2.0",
  "description": "Shared TypesScript config",
  "main": "tsconfig.json",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lbwa/tsconfig.git"
  },
  "author": "Liu Bowen <asyncmode@gmail.com>",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/lbwa/tsconfig/issues"
  },
  "homepage": "https://github.com/lbwa/tsconfig#readme",
  "keywords": [
    "tsconfig",
    "typescript",
    "ts"
  ],
  "files": [
    "tsconfig.json"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,sass,less,md}\""
  },
  "devDependencies": {
    "@lbwa/prettier-config": "^0.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@types/react": ">=17.0.5",
    "react": ">=17.0.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4 || ^5.3.3"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "prettier": "@lbwa/prettier-config",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,sass,less,md}": [
      "prettier --write"
    ]
  }
